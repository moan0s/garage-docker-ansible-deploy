{% if garage_garage_container_labels_traefik_enabled %}
{% if garage_garage_container_labels_traefik_docker_network %}
traefik.docker.network={{ garage_garage_container_labels_traefik_docker_network }}
{% endif %}
traefik.enable=true
traefik.http.routers.garage-{{ item.name }}-rpc.rule=Host("{{ item.rpc_public_addr }}")
traefik.http.routers.garage-{{ item.name }}-rpc.entrypoints={{ garage_garage_container_labels_traefik_entrypoints }}
traefik.http.routers.garage-{{ item.name }}-rpc.service=garage-{{ item.name }}-rpc
traefik.http.routers.garage-{{ item.name }}-rpc.tls.certResolver={{ garage_garage_container_labels_traefik_tls_certResolver }}
traefik.http.services.garage-{{ item.name }}-rpc.loadbalancer.server.port={{ item.rpc_port }}
{% endif %}
{% if item.s3_api_root_domain is defined  %}
traefik.http.routers.garage-{{ item.name }}-api.rule=Host("{{ item.s3_api_root_domain }}")
traefik.http.routers.garage-{{ item.name }}-api.service=garage-{{ item.name }}-api
traefik.http.routers.garage-{{ item.name }}-api.entrypoints={{ garage_garage_container_labels_traefik_entrypoints }}
traefik.http.routers.garage-{{ item.name }}-api.tls.certResolver={{ garage_garage_container_labels_traefik_tls_certResolver }}
traefik.http.services.garage-{{ item.name }}-api.loadbalancer.server.port={{ item.api_port }}
{% endif %}
{% if item.admin_root_domain is defined %}
traefik.http.routers.garage-{{ item.name }}-admin.rule=Host("{{ item.admin_root_domain }}")
traefik.http.routers.garage-{{ item.name }}-admin.service=garage-{{ item.name }}-admin
traefik.http.routers.garage-{{ item.name }}-admin.entrypoints={{ garage_garage_container_labels_traefik_entrypoints }}
traefik.http.routers.garage-{{ item.name }}-admin.tls.certResolver={{ garage_garage_container_labels_traefik_tls_certResolver }}
traefik.http.services.garage-{{ item.name }}-admin.loadbalancer.server.port={{ item.admin_port }}
{% endif %}

{{ garage_garage_container_labels_additional_labels }}
